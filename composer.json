{
    "name": "campusbokhandeln/prello",
    "description": "Prello.",
    "keywords": ["framework", "laravel", "laravel zero", "console", "cli"],
    "homepage": "https://github.com/campusbokhandeln/prello",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/campusbokhandeln/prello/issues",
        "source": "https://github.com/campusbokhandeln/prello"
    },
    "authors": [
        {
            "name": "Campus",
            "email": "tech@campusbokhandeln.se"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "guzzlehttp/guzzle": "^7.4",
        "illuminate/http": "^8.0",
        "illuminate/log": "^8.0",
        "laminas/laminas-text": "^2.8",
        "laravel-zero/phar-updater": "^1.0.6",
        "nunomaduro/laravel-console-menu": "^3.2"
    },
    "require-dev": {
        "laravel-zero/framework": "^8.8",
        "mockery/mockery": "^1.4.3",
        "pestphp/pest": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["builds/prello"]
}
